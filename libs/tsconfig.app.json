{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "target": "es2015",
    "module": "es2015",
    "moduleResolution": "node",
    "newLine": "lf",
    "declaration": true,
    "sourceMap": true,
    "inlineSources": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "skipLibCheck": true,
    "stripInternal": false,
    "noUnusedParameters": false,
    "strictNullChecks": false,
    "rootDir": ".",
    "outDir": "./../dist/out-tsc/libs",
    "types": [],
    "lib": [
      "es2015",
      "dom",
      "es2016.array.include",
      "es2017"
    ]
  },
  "exclude": [
    "**/*.spec.ts"
  ]
}
